
public class Fluxo {

	public static void main(String[] args) {
		System.out.println("Inicio do Main");
		metodo1();
		System.out.println("Final do Main");
	}
	
	
		public static void metodo1() {
			System.out.println("Inicio do metodo1");
			metodo2();
			System.out.println("Fim do metodo1");
		}
	}
	

